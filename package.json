    "framer-motion": "^12.27.0",
    "lucide": "^0.562.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-countdown-circle-timer": "^3.2.1",
